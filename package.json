{
  "name": "qa-app-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --dest dist/lib --target lib --name qa ./src/lib.js --mode production",
    "build-watch": "vue-cli-service build --target lib --watch --name qa ./src/lib.js --mode development",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.12.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "eslint-plugin-vue": "^7.10.0",
    "swagger-ui": "^3.50.0",
    "swagger-ui-dist": "^3.50.0",
    "vue-resource": "^1.5.2",
    "vue-router": "^3.5.1",
    "vuex": "^3.6.2",
    "vue-template-compiler": "^2.6.14"
  }
}
